// .vscode/c_cpp_properties.json
{
  "configurations": [
    {
      "name": "Win32",
      "includePath": [
        "${workspaceFolder}/src",
        "${workspaceFolder}/src/restore",
        "C:/tools/vcpkg/installed/x64-windows/include",    // vcpkg OpenCV headers
        "C:/opencv/build/include"                          // if you used prebuilt
      ],
      "defines": [ "_DEBUG", "UNICODE" ],
      "compilerPath": "C:/Program Files (x86)/Microsoft Visual Studio/2022/BuildTools/VC/Tools/MSVC/14.44.35207/bin/Hostx64/x64/cl.exe",
      "cStandard": "c17",
      "cppStandard": "c++17",
      "intelliSenseMode": "windows-msvc-x64"
    }
  ],
  "version": 4
}
